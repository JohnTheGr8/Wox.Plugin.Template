{
    "$schema": "http://json.schemastore.org/template",
    "author": "Ioannis G.",
    "classifications": [ "Library", "Plugin", "Wox" ],
    "name": "Plugin Library for the Wox launcher",
    "identity": "Wox.Plugin",
    "shortName": "wox-plugin",
    "tags": {
      "language": "C#",
      "type":"project"
    },
    "sourceName": "MyWoxPlugin",
    "preferNameDirectory": false,
    "symbols":{
      "pluginId": {
        "type": "generated",
        "generator": "guid",
        "replaces":"PluginId",
        "parameters": {
            "format":"N"
        }
      },
      "author": {
        "type": "parameter",
        "defaultValue": "AuthorName",
        "replaces":"PluginAuthor"
      },
      "keyword": {
        "type": "parameter",
        "defaultValue": "*",
        "replaces":"PluginActionKeyword"
      }
    }
}